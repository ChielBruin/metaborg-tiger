module Functions

imports Types
imports Occurrences

context-free syntax

  Dec.FunDecs = <<{FunDec "\n"}+>> {longest-match}
  
  FunDec.GenDec = <
    generator <Occ>(<{FArg ", "}*>) : <Type> = 
      <Exp>
  >
  
  FunDec.ProcDec = <
    function <Occ>(<{FArg ", "}*>) = 
      <Exp>
  >
  
  FunDec.FunDec = <
    function <Occ>(<{FArg ", "}*>) : <Type> = 
      <Exp>
  >

  FArg.FArg = <<Occ> : <Type>>
  
  Exp.Call = <<Occ>(<{Exp ", "}*>)> 
  Exp.Yield = <yield <Exp>>   
