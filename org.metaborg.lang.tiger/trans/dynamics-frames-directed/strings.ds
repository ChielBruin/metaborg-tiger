module dynamics-frames-directed/strings

imports ds-signatures/Strings-sig
imports dynamics-frames-directed/base
imports dynamics-frames-directed/natives
imports dynamics-frames-directed/numbers

signature
  constructors
    StringV : String -> V 
    
rules
 
  default(STRING()) --> UndefV()
 
  String(s) --> StringV(s')
  where
    unescapeS(s) => s'
  
  Lt(StringV(i), StringV(j)) --> IntV(ltS(i, j))
  
  Leq(StringV(i), StringV(j)) --> IntV(leqS(i, j))