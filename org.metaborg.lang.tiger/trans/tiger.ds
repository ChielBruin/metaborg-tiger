imports dynamics/equality

imports dynamics/nabl2-link
  
signature
  arrows
    Module -init-> V
    Module --> V
    
rules

  mod@Mod(_) -init-> vv
  where
    initNaBL2(mod) --> NaBL2;
    initHeap(mod) --> H;
    T nabl2_topIndexOf(mod), NaBL2 |- mod :: H, B OK() --> vv

  mod@Mod(e) --> vv
  where
    mod -paramsOf-> scope;
    initDefault(scope, {}) --> F;
    F |- stdLib() --> _;
    F |- e --> vv
