module dynamics/frames-native/strings

imports ds-signatures/Strings-sig
imports dynamics/frames-native/nabl2/nabl2-link
imports dynamics/frames-native/base
imports dynamics/frames-native/natives
imports dynamics/frames-native/numbers

signature
  constructors
    StringV : String -> V
    
rules
 
  default(STRING()) --> UndefV()
 
  String(s) --> StringV(s')
  where
    unescapeS(s) => s'
  
  Lt(StringV(i), StringV(j)) --> IntV(ltS(i, j))
  
  Leq(StringV(i), StringV(j)) --> IntV(leqS(i, j))