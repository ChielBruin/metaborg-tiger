module dynamic-semantics

imports
  exp
  records
  flowexp
  binexp
  letexp
  callexp
  arrays
  strings
  dnx-lib
  
signature
  link-labels
    P
    I
  continuations
    ret

  namespaces
    Var
    Field
    Loop
    
rules
  eval(Mod(exp)) =
   eval-instr(exp);
   exitOK()
   
  eval-instr(exp:UNIT()) = 
  	v1 <- eval-exp(exp)
  	
  eval-instr(exp:INT()) = 
    v1 <- eval-exp(exp);
    printc(iadd(int(48), v1))
