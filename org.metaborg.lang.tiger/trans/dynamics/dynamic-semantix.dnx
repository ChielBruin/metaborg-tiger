module dynamic-semantics

imports
  exp
  records
  flowexp
  binexp
  letexp
  callexp
  arrays
  strings
  common
  
signature
  link-labels
    P
    I
  continuations
    ret

  namespaces
    Var
    Field
   
rules
  eval:: ast -> instr
  eval(Mod(exp)) =
   eval-instr(exp);
   exitOK()  
   
  eval-instr:: ast -> instr
  eval-instr(exp:UNIT()) = eval-unit-exp(exp)
  eval-instr(exp) = 
    v1 <- eval-exp(exp);
    printc(iadd(int(48), v1))
